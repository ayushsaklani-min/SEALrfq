# Environment variables for SealRFQ Phase 0 testing
# Copy this file to .env and fill in your actual Aleo private keys

# Buyer account (creates RFQ, selects winner, funds escrow)
BUYER_KEY=APrivateKey1...

# Vendor 1 account (submits bid)
VENDOR1_KEY=APrivateKey1...

# Vendor 2 account (submits bid)
VENDOR2_KEY=APrivateKey1...

# Vendor 2 address (for payment recipient)
VENDOR2_ADDRESS=aleo1...

# Deployer account (can be same as BUYER_KEY)
DEPLOYER_KEY=APrivateKey1...

# Aleo network configuration
ALEO_NETWORK=testnet
ALEO_RPC_URL=https://api.explorer.aleo.org/v1
ALEO_EXPLORER_URL=https://explorer.aleo.org
